pragma solidity ^0.8.24;
contract Operations {
    function compute() external pure {
        assembly {
            function add(a, b) -> result {
                result := add(a, b)
            }
            function subtract(a, b) -> result {
                result := sub(a, b)
            }
            function multiply(a, b) -> result {
                result := mul(a, b)
            }
        }
    }
}
